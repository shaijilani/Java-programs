class Main {
    public static void main(String[] args) {
        String str = "MARKRAM";
        String st = "";
        for(int i =str.length()-1; i>=0; i--){
            st = st+str.charAt(i);
        }
        System.out.println(st);
        if(str.equals(st)){
            System.out.println("palindrom");
        }
        else{
            System.out.println("Not palindrom");
        }
    }
}
