class A{
    public void show()
    {
        System.out.println("In A class");
    }
    class B{
        public void display()
        {
            System.out.println("Ininner class");
        }
    }
}
class Main {
    public static void main (String args [])
    {
        A obj = new A();
        obj.show();
        
        A.B obj1 = obj.new B();
        obj1.display();
    }
}
