class Main {
    public static void main(String[] args) {
        int arr[] = {94,45,63,84,96};
        int large = arr[0];
        for(int i =1; i<arr.length;i++)
        {
            if(arr[i]>=large)
            {
                large =arr[i];
            }
        }
        System.out.println("From the given array the largest number is " + large);
    }
    
}
