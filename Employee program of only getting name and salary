class Employee{
    String Name;
    int Id;
    int No;
    int Salary;
    
        void getName() {
        
            System.out.println(Name);
        }
        void getId() {
            System.out.println(Id);
        }
        void getNO( ){
            System.out.println(No);
        }
        void getSalary() {
            System.out.println(Salary);
        }
        
    }

class Main{
    public static void main(String args[]){
        Employee E=new Employee();
        E.Name="jilani";
        E.Id=7;
        E.No=21;
        E.Salary=100000;
        E.getName();
        E.getSalary();
    }
}
