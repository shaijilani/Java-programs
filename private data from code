class Mobile{
    private int age ;
    private String name;
    
    
    public int getAge(){
        return age;
    }
    
    public String getName(){
        return name;
    }
    
    public void setAge(int a)
    {
        age =a;
    }
    public void setName(String n)
    {
        name = n;
    }
}
    class Main{
        public static void main (String args[]){
            Mobile obj =new Mobile();
            obj.setAge(30);
            obj.setName("jilani");
            
             System.out.println(obj.getName() + ":" + obj.getAge());
            
            
        }
    }
