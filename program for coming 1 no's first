class Main{
	public static void main(String[] args) {
		int arr[] ={1,1,0,1,0,1};
		int i=1;
		int j=1;
		while(i<arr.length){
		    if(arr[i]==1){
		        arr[j]=1;
		        i++;
		        j++;
		    
		}
		else{
		    i++;
		}
		}
		for(int k=j; k<arr.length; k++){
		   arr[k]=0;
		}
		for(int l=0; l<arr.length;l++){
		    System.out.print(arr[l] + " ");
		}
	}
}
